{
  "name": "krpc-web",
  "version": "1.0.0",
  "description": "KRPC Web client for krpc,fetch first then xhr",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest --coverage --ci --reporters=default --reporters=jest-junit --detectOpenHandles",
    "format": "prettier --write \"src/**/*.*s\"",
    "prebuild": "npm run format",
    "build": "tsc",
    "postbuild": "copyfiles -u 1 src/internal_* dist/",
    "localTest": "npm run build && jest --coverage"
  },
  "keywords": [
    "grpc",
    "krpc-web"
  ],
  "author": "martin.cong",
  "license": "UNLICENSED",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^14.18.63",
    "copyfiles": "^2.4.1",
    "grpc-web": "~1.5.0",
    "jest": "^27.5.1",
    "jest-junit": "^13.2.0",
    "node-fetch": "^3.3.2",
    "prettier": "^2.8.8",
    "ts-jest": "^27.1.5",
    "ts-node": "^8.10.2",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "whatwg-fetch": "^3.6.20"
  },
  "dependencies": {
    "krpc-base": "^1.0.0",
    "class-validator": "^0.13.2"
  },
  "eslintIgnore": [
    "src/internal_pb.d.ts"
  ],
  "directories": {
    "example": "example"
  }
}
